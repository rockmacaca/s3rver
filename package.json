{
  "name": "s3rver",
  "version": "3.6.1",
  "description": "Fake S3 server for node",
  "keywords": [
    "fake",
    "s3",
    "server",
    "mock",
    "false",
    "amazon"
  ],
  "author": {
    "name": "Jamie Hall",
    "email": "hello@jamiehall.eu",
    "url": "http://github.com/jamhall/s3rver"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:jamhall/s3rver.git"
  },
  "bugs": {
    "url": "https://github.com/jamhall/s3rver/issues"
  },
  "homepage": "https://github.com/jamhall/s3rver",
  "scripts": {
    "test": "mocha",
    "eslint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier \"**\"",
    "format": "npm run eslint -- --fix && npm run prettier -- --write"
  },
  "main": "lib/s3rver.js",
  "files": [
    "bin/**/*",
    "lib/**/*",
    "example/*.xml"
  ],
  "bin": {
    "s3rver": "bin/s3rver.js"
  },
  "directories": {
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "dependencies": {
    "@koa/router": "^10.0.0",
    "busboy": "^0.3.1",
    "commander": "^7.0.0",
    "fast-xml-parser": "^3.12.19",
    "fs-extra": "^9.0.0",
    "he": "^1.2.0",
    "koa": "^2.7.0",
    "koa-logger": "^3.2.0",
    "lodash": "^4.17.5",
    "statuses": "^2.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "aws-sdk": "2.842.0",
    "chai": "4.3.0",
    "chai-as-promised": "7.1.1",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.1.0",
    "express": "4.17.1",
    "form-data": "3.0.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "md5": "2.3.0",
    "mocha": "8.3.0",
    "moment": "2.29.1",
    "p-map": "4.0.0",
    "prettier": "2.2.1",
    "request": "2.88.2",
    "request-promise-native": "1.0.9"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
